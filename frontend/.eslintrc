{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-debugger": 1,
    "no-invalid-regexp": 1,
    "array-callback-return": 1,
    "eqeqeq": 1,
    "no-else-return": 1,
    "no-eval": 1,
    "no-fallthrough": 1,
    "no-implied-eval": 1,
    "no-multi-spaces": 1,
    "no-return-await": 1,
    "no-return-assign": 1,
    "no-redeclare": 1,
    "no-undef": 1,
    "no-lonely-if": 1,
    "no-unneeded-ternary": 1,
    "object-curly-spacing": 0,
    "array-bracket-spacing": 0,
    "keyword-spacing": [1, { "before": true }]
  }
}
